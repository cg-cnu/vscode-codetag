{
  "name": "vscode-codetags",
  "displayName": "codetags",
  "description": "Add tags to your code with predefined templates",
  "version": "0.1.0",
  "publisher": "cg-cnu",
  "engines": {
    "vscode": "^1.16.0"
  },
  "author": {
    "email": "sreenivas9alapati@gmail.com",
    "name": "sreenivas alapati",
    "url": "https://twitter.com/cgcnu"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:codetags.tags"
  ],
  "bugs": {
    "email": "sreenivas9alapati@gmail.com",
    "url": "https://github.com/cg-cnu/vscode-codetags/issues"
  },
  "galleryBanner": {
    "color": "#041A31",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cg-cnu/vscode-codetags"
  },
  "readme": "README.md",
  "icon": "icons/codetags-128.png",
  "license": "MIT",
  "keywords": [
    "code",
    "tags",
    "codetags",
    "codetag",
    "code tags"
  ],
  "homepage": "https://github.com/cg-cnu/vscode-codetags",
  "main": "./out/src/codetags",
  "contributes": {
    "configuration": [
      {
        "properties": {
          "codetags.date": {
            "type": "object",
            "title": "date",
            "description": "codetag date settings",
            "properties": {
              "enable": {
                "type": "boolean",
                "default": true,
                "title": "enable",
                "description": "enable date in codetag"
              },
              "format": {
                "type": "string",
                "default": "yyyy-MM-dd",
                "title": "format",
                "description": "specify date-fns style date format"
              }
            }
          },
          "codetags.user": {
            "type": "object",
            "title": "user",
            "description": "codetag user settings",
            "properties": {
              "enable": {
                "type": "boolean",
                "title": "enable",
                "default": true,
                "description": "enable user name in codetag"
              },
              "name": {
                "type": "string",
                "title": "name",
                "description": "set default user"
              }
            }
          },
          "codetags.default": {
            "type": "boolean",
            "title": "Default",
            "default": true,
            "description": "enable or disable default tags"
          },
          "codetags.custom": {
            "type": "array",
            "description": "add custom codetags",
            "items": {
              "type": "object",
              "title": "CodeTag",
              "description": "custom codetag structure",
              "required": [
                "name",
                "description"
              ],
              "properties": {
                "name": {
                  "type": "string",
                  "title": "Name",
                  "description": "codetag name"
                },
                "label": {
                  "type": "string",
                  "title": "Label",
                  "description": "codetag label. Used in vscode UI"
                },
                "description": {
                  "type": "string",
                  "title": "Description",
                  "description": "codetag description"
                }
              }
            }
          }
        }
      }
    ],
    "commands": [
      {
        "command": "codetags.tags",
        "title": "codetags"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^11.15.12",
    "mocha": "^10.2.0",
    "typescript": "^3.8.3",
    "vscode": "^1.1.37"
  },
  "dependencies": {
    "date-fns": "^2.12.0",
    "git-user-name": "^2.0.0"
  }
}